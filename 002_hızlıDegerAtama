#include<stdio.h>

int main(){
    int i=3;

    i+=50;
    printf("%d\n",i);

    i-=5;
    printf("%d\n",i);

    i*=5;
    printf("%d\n",i);

    
    i/=3;
    printf("%d\n",i);
    
    i%=3;
    printf("%d\n",i);

    print("%d\n",);
    
    printf("%d\n",i++); // önce yazdırıp sonra değer artırır 
    printf("%d\n",++i); // önce degerartırıp sonra yazdırır

  return 0;





}
